@prefix : <http://loop3d.org/GSO/ontology/2020/1/geologicfault/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://loop3d.org/GSO/ontology/2020/1/geologicfault/ontology> .

<http://loop3d.org/GSO/ontology/2020/1/geologicfault/ontology> rdf:type owl:Ontology ;
                                                                owl:imports <http://loop3d.org/GSO/ontology/2020/1/common/ontology> ,
                                                                            <http://loop3d.org/GSO/ontology/2020/1/geologicprocess/ontology> ,
                                                                            <http://loop3d.org/GSO/ontology/2020/1/geologicquality/ontology> ,
                                                                            <http://loop3d.org/GSO/ontology/2020/1/geologicstructure/ontology> ,
                                                                            <http://loop3d.org/GSO/ontology/2020/1/geology/ontology> ,
                                                                            <http://loop3d.org/GSO/ontology/2020/1/quality/ontology> ;
                                                                rdfs:comment "This file contains the 2016 SKOS-RDF version of the CGI Fault Type, Fault Movement Sense and Fault Movement Type vocabulary. Compilation and review in MS Excel spreadsheet, converted to MS Excel for SKOS generation using SKOS_for_GA_from_XLS_2016.03.xslt"@en ,
                                                                             "This ontology includes extensions for describing faults in more detail. (ie, a summary of movement type and orientation) as defined by the IUGS Commission for Geoscience Information (CGI) Geoscience Terminology Working Group. Although the terms in this vocabulary are in common usage with definitions in standard texts or glossaries like Neuendorf et al. (2005), the definitions here attempt to establish unambiguous criteria to distinguish the categories, and are thus listed with this vocabulary as the source."@en ,
                                                                             "dct:creator--  CGI Geoscience Terminology Working Group; OWL conversion by Stephen Richard for Loop3D project"@en ,
                                                                             "dct:date--  2016-10-19"@en ,
                                                                             "dct:modified-- 2020-01-29"@en ;
                                                                rdfs:label "Fault"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/source
<http://purl.org/dc/terms/source> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://loop3d.org/GSO/ontology/2020/1/common/contains
<http://loop3d.org/GSO/ontology/2020/1/common/contains> rdf:type owl:ObjectProperty .


###  http://loop3d.org/GSO/ontology/2020/1/common/hasEssentialPart
<http://loop3d.org/GSO/ontology/2020/1/common/hasEssentialPart> rdf:type owl:ObjectProperty .


###  http://loop3d.org/GSO/ontology/2020/1/common/hasQuality
<http://loop3d.org/GSO/ontology/2020/1/common/hasQuality> rdf:type owl:ObjectProperty .


###  http://loop3d.org/GSO/ontology/2020/1/common/hostedBy
<http://loop3d.org/GSO/ontology/2020/1/common/hostedBy> rdf:type owl:ObjectProperty .


###  http://loop3d.org/GSO/ontology/2020/1/common/isOutputOf
<http://loop3d.org/GSO/ontology/2020/1/common/isOutputOf> rdf:type owl:ObjectProperty .


###  http://loop3d.org/GSO/ontology/2020/1/common/isQualityOf
<http://loop3d.org/GSO/ontology/2020/1/common/isQualityOf> rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://loop3d.org/GSO/ontology/2020/1/common/Material_Feature
<http://loop3d.org/GSO/ontology/2020/1/common/Material_Feature> rdf:type owl:Class .


###  http://loop3d.org/GSO/ontology/2020/1/common/Physical_Quality
<http://loop3d.org/GSO/ontology/2020/1/common/Physical_Quality> rdf:type owl:Class .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Detachment_Fault
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Detachment_Fault> rdf:type owl:Class ;
                                                                       rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicfault/Low_Angle_Normal_Fault> ;
                                                                       <http://purl.org/dc/terms/source> "CGI faulttype SKOS vocabulary 2016-10-19"@en ;
                                                                       rdfs:comment "A regional-scale, large displacement, low-angle normal fault."@en ;
                                                                       rdfs:label "detachment fault"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Dextral_Strike_Slip_Fault
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Dextral_Strike_Slip_Fault> rdf:type owl:Class ;
                                                                                rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicfault/Strike_Slip_Fault> ;
                                                                                <http://purl.org/dc/terms/source> "CGI faulttype SKOS vocabulary 2016-10-19"@en ;
                                                                                rdfs:comment "Fault with right-lateral strike-parallel displacement component of slip vector more than 10 times the dip-parallel component of the slip vector at at least one location along the fault, and right-lateral displacement over more than half the mapped trace of the fault."@en ;
                                                                                rdfs:label "dextral strike slip fault"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Dip_Separation
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Dip_Separation> rdf:type owl:Class ;
                                                                     rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault_Separation> ;
                                                                     <http://purl.org/dc/terms/source> "GGIC vocabularies working group (Australia)"@en ;
                                                                     rdfs:comment "A fault along which there is some separation parallel to the dip of the fault."@en ;
                                                                     rdfs:label "dip separation sense"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Dip_Slip
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Dip_Slip> rdf:type owl:Class ;
                                                               rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault_Slip> ;
                                                               <http://purl.org/dc/terms/source> "GGIC vocabularies working group (Australia)"@en ;
                                                               rdfs:comment "The net slip of the fault lies in the dip direction of the fault."@en ;
                                                               rdfs:label "dip slip"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Ductile_Shear_Zone
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Ductile_Shear_Zone> rdf:type owl:Class ;
                                                                         rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault_Zone> ,
                                                                                         [ rdf:type owl:Restriction ;
                                                                                           owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasEssentialPart> ;
                                                                                           owl:someValuesFrom [ owl:intersectionOf ( <http://loop3d.org/GSO/ontology/2020/1/geology/Rock_Body>
                                                                                                                                     [ rdf:type owl:Restriction ;
                                                                                                                                       owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/isOutputOf> ;
                                                                                                                                       owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/geologicprocess/Ductile_Flow>
                                                                                                                                     ]
                                                                                                                                   ) ;
                                                                                                                rdf:type owl:Class
                                                                                                              ]
                                                                                         ] ;
                                                                         rdfs:comment "a broadly planar zone of shear displacement within which deformation has occurred without loss of material continuity"@en ;
                                                                         rdfs:label "Ductile Shear Zone"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Extraction_Fault
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Extraction_Fault> rdf:type owl:Class ;
                                                                       rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault> ;
                                                                       <http://purl.org/dc/terms/source> "Froitzheim, N., Pleuger, J. & Nagel, T.J., 2006. Extraction faults. Journal of Structural Geology v28, pp. 1388-1395."@en ;
                                                                       rdfs:comment "A fault whose two sides have approached each other substantially in the direction perpendicular to the fault."@en ;
                                                                       rdfs:label "extraction fault"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault> rdf:type owl:Class ;
                                                            rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicstructure/Fracture> ,
                                                                            [ rdf:type owl:Restriction ;
                                                                              owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasQuality> ;
                                                                              owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/quality/Displacement>
                                                                            ] ;
                                                            <http://purl.org/dc/terms/source> "this vocabulary"@en ;
                                                            rdfs:comment "A discrete surface, or zone of discrete surfaces, with some thickness, separating two rock masses across which one mass has slid past the other and characterized by brittle deformation."@en ;
                                                            rdfs:label "Fault"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault_Movement_Magnitude
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault_Movement_Magnitude> rdf:type owl:Class ;
                                                                               rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/common/Physical_Quality> ,
                                                                                               [ rdf:type owl:Restriction ;
                                                                                                 owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/isQualityOf> ;
                                                                                                 owl:allValuesFrom <http://loop3d.org/GSO/ontology/2020/1/quality/Displacement>
                                                                                               ] ;
                                                                               rdfs:comment "magnitude of slip or separation"@en ;
                                                                               rdfs:label "Displacement magnitude"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault_Movement_Sense
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault_Movement_Sense> rdf:type owl:Class ;
                                                                           rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/common/Physical_Quality> ,
                                                                                           [ rdf:type owl:Restriction ;
                                                                                             owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/isQualityOf> ;
                                                                                             owl:allValuesFrom <http://loop3d.org/GSO/ontology/2020/1/quality/Displacement>
                                                                                           ] ;
                                                                           rdfs:comment "Quality that specifies the relative sense of displacement (right, left, up, down) between rocks on opposites of a fault."@en ;
                                                                           rdfs:label "Fault movement sense "@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault_Movement_Vector
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault_Movement_Vector> rdf:type owl:Class ;
                                                                            rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/common/Physical_Quality> ,
                                                                                            [ rdf:type owl:Restriction ;
                                                                                              owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasQuality> ;
                                                                                              owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/geologicquality/Azimuth>
                                                                                            ] ,
                                                                                            [ rdf:type owl:Restriction ;
                                                                                              owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasQuality> ;
                                                                                              owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/geologicquality/Plunge>
                                                                                            ] ,
                                                                                            [ rdf:type owl:Restriction ;
                                                                                              owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/isQualityOf> ;
                                                                                              owl:allValuesFrom <http://loop3d.org/GSO/ontology/2020/1/quality/Displacement>
                                                                                            ] ;
                                                                            rdfs:comment "Slip vector linking piercing point on opposite sides of fault, or separation vector linking offset surface along fault trace."@en ;
                                                                            rdfs:label "Movement Vector"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault_Separation
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault_Separation> rdf:type owl:Class ;
                                                                       rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/common/Physical_Quality> ,
                                                                                       [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/isQualityOf> ;
                                                                                         owl:allValuesFrom <http://loop3d.org/GSO/ontology/2020/1/quality/Displacement>
                                                                                       ] ;
                                                                       rdfs:comment "a vector (magnitude and orientation) specifying the distance between the intersection of a surface on opposite sides of a displacment surface. In 3-D is the projection of a vector normal to the displaced surface into the displacment surface, but might also be specified as offset of the intersection of displacement surface and marker surface in some other profile view (e.g. horizontal/map view, or a cross section)"@en ;
                                                                       rdfs:label "Separation"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault_Slip
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault_Slip> rdf:type owl:Class ;
                                                                 rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/common/Physical_Quality> ,
                                                                                 [ rdf:type owl:Restriction ;
                                                                                   owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/isQualityOf> ;
                                                                                   owl:allValuesFrom <http://loop3d.org/GSO/ontology/2020/1/quality/Displacement>
                                                                                 ] ;
                                                                 rdfs:comment "a vector (magnitude and orientation) that links a peircing point on opposite sides of a displacement surface."@en ;
                                                                 rdfs:label "Slip"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault_Zone
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault_Zone> rdf:type owl:Class ;
                                                                 rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/common/Material_Feature> ,
                                                                                 [ rdf:type owl:Restriction ;
                                                                                   owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/contains> ;
                                                                                   owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault>
                                                                                 ] ,
                                                                                 [ rdf:type owl:Restriction ;
                                                                                   owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasEssentialPart> ;
                                                                                   owl:someValuesFrom [ owl:intersectionOf ( <http://loop3d.org/GSO/ontology/2020/1/geology/Rock_Body>
                                                                                                                             [ rdf:type owl:Restriction ;
                                                                                                                               owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/isOutputOf> ;
                                                                                                                               owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/geology/Deformation>
                                                                                                                             ]
                                                                                                                           ) ;
                                                                                                        rdf:type owl:Class
                                                                                                      ]
                                                                                 ] ,
                                                                                 [ rdf:type owl:Restriction ;
                                                                                   owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasQuality> ;
                                                                                   owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/quality/Thickness>
                                                                                 ] ,
                                                                                 [ rdf:type owl:Restriction ;
                                                                                   owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hostedBy> ;
                                                                                   owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault>
                                                                                 ] ;
                                                                 rdfs:comment "A material spatial feature that contains one or more fault surfaces as well as deformed rocks related to the faults located between and adjacent to the fault traces"@en ;
                                                                 rdfs:label "Fault Zone "@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Generic_Decollement_Fault
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Generic_Decollement_Fault> rdf:type owl:Class ;
                                                                                rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicfault/Low_Angle_Fault> ;
                                                                                <http://purl.org/dc/terms/source> "Neuendorf et al. 2005"@en ;
                                                                                rdfs:comment "A large-displacement (kilometers or tens of kilometers) shallowly dipping to subhorizontal fault or shear zone."@en ;
                                                                                rdfs:label "decollement"@fr ,
                                                                                           "generic decollement"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/High_Angle_Fault
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/High_Angle_Fault> rdf:type owl:Class ;
                                                                       rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault> ;
                                                                       <http://purl.org/dc/terms/source> "CGI faulttype SKOS vocabulary 2016-10-19"@en ;
                                                                       rdfs:comment "Fault that dips at least 45 degrees over more than half of its recognized extent, for which slip or separation is not explicitly specified."@en ;
                                                                       rdfs:label "high angle fault"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/High_Angle_Normal_Fault
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/High_Angle_Normal_Fault> rdf:type owl:Class ;
                                                                              rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicfault/High_Angle_Fault> ,
                                                                                              <http://loop3d.org/GSO/ontology/2020/1/geologicfault/Normal_Fault> ;
                                                                              <http://purl.org/dc/terms/source> "CGI faulttype SKOS vocabulary 2016-10-19"@en ;
                                                                              rdfs:comment "Fault that dips at least 45 degrees over more than half of the recognized extent of the fault with the hanging wall displaced from a structurally higher position relative to footwall rocks."@en ;
                                                                              rdfs:label "high angle normal fault"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/High_Angle_Reverse_Fault
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/High_Angle_Reverse_Fault> rdf:type owl:Class ;
                                                                               rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicfault/High_Angle_Fault> ,
                                                                                               <http://loop3d.org/GSO/ontology/2020/1/geologicfault/Reverse_Fault> ;
                                                                               <http://purl.org/dc/terms/source> "CGI faulttype SKOS vocabulary 2016-10-19"@en ;
                                                                               rdfs:comment "Reverse fault that dips at least 45 degrees over more than half of its recognized extent, for which slip or separation is not explicitly specified."@en ;
                                                                               rdfs:label "high angle reverse"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Horizontal_Fault
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Horizontal_Fault> rdf:type owl:Class ;
                                                                       rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicfault/Low_Angle_Fault> ;
                                                                       <http://purl.org/dc/terms/source> "GGIC vocabularies working group (Australia)"@en ;
                                                                       rdfs:comment "Fault that dips less than 10 degrees over more than half the recognized extent of the fault."@en ;
                                                                       rdfs:label "horizontal fault"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Left_Normal_Fault
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Left_Normal_Fault> rdf:type owl:Class ;
                                                                        rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicfault/Oblique_Slip_Fault> ;
                                                                        <http://purl.org/dc/terms/source> "CGI faulttype SKOS vocabulary 2016-10-19"@en ;
                                                                        rdfs:comment "High angle fault with slip vector that has ratio of strike-parallel to dip-parallel displacement between 10 to 1 and 1 to 10 at at least one location along the mapped trace, with left-lateral strike-parallel component and normal dip-parallel component over at least half the mapped trace of the fault."@en ;
                                                                        rdfs:label "left normal fault"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Left_Reverse_Fault
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Left_Reverse_Fault> rdf:type owl:Class ;
                                                                         rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicfault/Oblique_Slip_Fault> ;
                                                                         <http://purl.org/dc/terms/source> "CGI faulttype SKOS vocabulary 2016-10-19"@en ;
                                                                         rdfs:comment "High angle fault with slip vector that has ratio of strike-parallel to dip-parallel displacement between 10 to 1 and 1 to 10 at at least one location along the mapped trace, with left-lateral strike-parallel component and reverse dip-parallel component over at least half the mapped trace of the fault."@en ;
                                                                         rdfs:label "left reverse fault"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Low_Angle_Fault
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Low_Angle_Fault> rdf:type owl:Class ;
                                                                      rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault> ;
                                                                      <http://purl.org/dc/terms/source> "CGI faulttype SKOS vocabulary 2016-10-19"@en ;
                                                                      rdfs:comment "Fault that dips less than 45 degrees over more than half of the recognized extent of the fault."@en ;
                                                                      rdfs:label "low angle fault"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Low_Angle_Normal_Fault
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Low_Angle_Normal_Fault> rdf:type owl:Class ;
                                                                             rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicfault/Low_Angle_Fault> ,
                                                                                             <http://loop3d.org/GSO/ontology/2020/1/geologicfault/Normal_Fault> ;
                                                                             <http://purl.org/dc/terms/source> "CGI faulttype SKOS vocabulary 2016-10-19"@en ;
                                                                             rdfs:comment "Fault that dips less than 45 degrees over more than half of the recognized extent of the fault with the hanging wall displaced from a structurally higher position relative to footwall rocks."@en ;
                                                                             rdfs:label "low angle normal fault"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Mixed_Extraction_Fault
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Mixed_Extraction_Fault> rdf:type owl:Class ;
                                                                             rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicfault/Extraction_Fault> ;
                                                                             <http://purl.org/dc/terms/source> "Froitzheim, N., Pleuger, J. & Nagel, T.J., 2006. Extraction faults. Journal of Structural Geology v28, pp. 1388-1395."@en ;
                                                                             rdfs:comment "An extraction fault with some displacement within the fault plane."@en ;
                                                                             rdfs:label "mixed extraction fault"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Normal_Fault
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Normal_Fault> rdf:type owl:Class ;
                                                                   rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault> ;
                                                                   <http://purl.org/dc/terms/source> "CGI faulttype SKOS vocabulary 2016-10-19"@en ;
                                                                   rdfs:comment "Fault with dip-parallel displacement component of slip vector more than 10 times the strike-parallel component of the slip vector over more than half recognized extent of the fault, and for which the fault dips consistently in the same direction, and for which the hanging wall has been displaced down relative to the footwall."@en ;
                                                                   rdfs:label "normal fault"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Oblique_Slip
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Oblique_Slip> rdf:type owl:Class ;
                                                                   rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicfault/Fault_Slip> ;
                                                                   <http://purl.org/dc/terms/source> "GGIC vocabularies working group (Australia), SMR"@en ;
                                                                   rdfs:comment "The net slip of the fault lies between the strike and dip directions of the fault, the slip vector rakes between 10 and 80 degrees in the plane of the fault."@en ;
                                                                   rdfs:label "oblique slip"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicfault/Oblique_Slip_Fault
<http://loop3d.org/GSO/ontology/2020/1/geologicfault/Oblique_Slip_Fault> rdf:type owl:Class ;
