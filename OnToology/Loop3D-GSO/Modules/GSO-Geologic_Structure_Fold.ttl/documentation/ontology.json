}, {
  "@id" : "http://loop3d.org/GSO/ontology/2020/1/geologicfold/Antiform",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://purl.org/dc/terms/source" : [ {
    "@value" : "https://inspire.ec.europa.eu/codelist/FoldProfileTypeValue/antiform"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "A convex-upward, concave downward fold."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Antiform"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
