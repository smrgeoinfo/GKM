  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://purl.org/dc/terms/source" : [ {
    "@value" : "https://inspire.ec.europa.eu/codelist/FoldProfileTypeValue/anticline"
  } ],
