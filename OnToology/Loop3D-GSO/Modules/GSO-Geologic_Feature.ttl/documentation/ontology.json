}, {
  "@id" : "http://loop3d.org/GSO/ontology/2020/1/geologicfeature/Lithophysae",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://purl.org/dc/terms/source" : [ {
    "@language" : "en",
    "@value" : "Neuendorf et al 2005"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "Bubble-like cavity with concentric shells of finely crystalline minerals, open space remains in core of structure. Radiating fibrous structure may be present in secondary mineral fill. Typically in silicic volcanic rock."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "lithophysae"
  } ],
