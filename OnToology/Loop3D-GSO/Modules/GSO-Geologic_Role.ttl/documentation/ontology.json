  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "Granular material role for constituent that forms less than about 5 percent of a Rock Material"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "accessory constituent"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://loop3d.org/GSO/ontology/2020/1/geologicrole/Crystal_Role"
  } ]
}, {
  "@id" : "http://loop3d.org/GSO/ontology/2020/1/geologicrole/Block",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://purl.org/dc/terms/source" : [ {
    "@language" : "en",
    "@value" : "CGI rockbodypart vocabulary 2016-11-21 "
  } ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "Geologic unit part is one or more masses with generally sharp boundaries and block-like geometry within a matrix of some other material emplaced by processes at the earth's surface--e.g. volcanic eruption or mass wasting. Use for objects that are larger that what would be considered a clast, e.g. greater than about 10 m shortest dimension, typically large enough and sparse enough to be considered individual objects."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "blocks"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://loop3d.org/GSO/ontology/2020/1/geologicrole/Inclusion"
  } ]
}, {
  "@id" : "http://loop3d.org/GSO/ontology/2020/1/geologicrole/Cataclastic_Matrix",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "rock consisting of greater than 50% sulfide or sulfosalt minerals formed by sedimentary exhalative processes."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Matrix formed by deformation "
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://loop3d.org/GSO/ontology/2020/1/geologicrole/Matrix"
  } ]
}, {
  "@id" : "http://loop3d.org/GSO/ontology/2020/1/geologicrole/Cement",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "mineral material introduced during diagenesis that binds detrital material togther in a clastic rock. "
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "cement "
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://loop3d.org/GSO/ontology/2020/1/geologicrole/Matrix"
  } ]
}, {
  "@id" : "http://loop3d.org/GSO/ontology/2020/1/geologicrole/Clast",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "Role for any fragment of material that is a constituent in a rock material; applies to fragments formed before lithification (e.g. sedimentary clasts) as well as fragments formed by in-place deformation of pre-existing rock (e.g. cataclastic fragments)."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Clast"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://loop3d.org/GSO/ontology/2020/1/common/Part"
  }, {
    "@id" : "_:genid1"
  }, {
    "@id" : "_:genid2"
  } ]
}, {
  "@id" : "http://loop3d.org/GSO/ontology/2020/1/geologicrole/Crystal_Role",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "constituent formed by crystallization from melt or by metamorphic processes. Distinct from constituent roles related to clastic processes, or diagenetic processes."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Crystal Constituent"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://loop3d.org/GSO/ontology/2020/1/common/Role"
  }, {
    "@id" : "_:genid3"
  }, {
    "@id" : "_:genid4"
  } ]
}, {
  "@id" : "http://loop3d.org/GSO/ontology/2020/1/geologicrole/Enclave",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://purl.org/dc/terms/source" : [ {
    "@language" : "en",
    "@value" : "Cobbing, 2000, Barbarin, 1991"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "General term for a polymineralic aggregate enclosed in a granitoid. Use when origin of inclusion as xenolith or a separate igneous phase is unclear."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "enclave"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://loop3d.org/GSO/ontology/2020/1/geologicrole/Inclusion"
  } ]
}, {
  "@id" : "http://loop3d.org/GSO/ontology/2020/1/geologicrole/Floating_Clast",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "Clastic particles that are embedded in a sedimentary matrix in the original depositional process, e.g. clasts in a matrix-supported debris flow "
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "floating clast "
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://loop3d.org/GSO/ontology/2020/1/geologicrole/Clast"
  } ]
}, {
  "@id" : "http://loop3d.org/GSO/ontology/2020/1/geologicrole/Framework_Clast",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "Clasts are part of an interconnected network of clasts in a sedimentary rock.  Conceptual model is the accumulation of clastic particles as originally deposited-- a heap of particles without matrix constituents. "
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "framework clast "
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://loop3d.org/GSO/ontology/2020/1/geologicrole/Clast"
  } ]
}, {
  "@id" : "http://loop3d.org/GSO/ontology/2020/1/geologicrole/Framework_Crystals",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "crystal that form the main framework of an igneous or metamorphic rock. Framework is the essential supporting structure of a rock material, formed by a collection of particles all in mutual contact to form a continuous connected framerwork; the framework commonly comprises more that one mineral phase, represented as different granular materials in this model, all in the 'framework crystal' role."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
