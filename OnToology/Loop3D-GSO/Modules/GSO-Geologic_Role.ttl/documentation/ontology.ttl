@prefix : <http://loop3d.org/GSO/ontology/2020/1/geologicrole/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://loop3d.org/GSO/ontology/2020/1/geologicrole/ontology> .

<http://loop3d.org/GSO/ontology/2020/1/geologicrole/ontology> rdf:type owl:Ontology ;
                                                               owl:imports <http://loop3d.org/GSO/ontology/2020/1/common/ontology> ,
                                                                           <http://loop3d.org/GSO/ontology/2020/1/geologicprocess/ontology> ,
                                                                           <http://loop3d.org/GSO/ontology/2020/1/geology/ontology> ;
                                                               rdfs:comment "dct:creator--  Stephen M Richard, B.Brodaric, based on CGI Geoscience Terminology Working Group"@en ,
                                                                            "dct:date--  2020-08-04"@en ;
                                                               rdfs:label "Geologic Role"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/source
<http://purl.org/dc/terms/source> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://loop3d.org/GSO/ontology/2020/1/common/hasRoleObject
<http://loop3d.org/GSO/ontology/2020/1/common/hasRoleObject> rdf:type owl:ObjectProperty .


###  http://loop3d.org/GSO/ontology/2020/1/common/hasRolePlayer
<http://loop3d.org/GSO/ontology/2020/1/common/hasRolePlayer> rdf:type owl:ObjectProperty .


###  http://loop3d.org/GSO/ontology/2020/1/common/isSampleOf
<http://loop3d.org/GSO/ontology/2020/1/common/isSampleOf> rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://loop3d.org/GSO/ontology/2020/1/common/Part
<http://loop3d.org/GSO/ontology/2020/1/common/Part> rdf:type owl:Class .


###  http://loop3d.org/GSO/ontology/2020/1/common/Role
<http://loop3d.org/GSO/ontology/2020/1/common/Role> rdf:type owl:Class .


###  http://loop3d.org/GSO/ontology/2020/1/common/Sample
<http://loop3d.org/GSO/ontology/2020/1/common/Sample> rdf:type owl:Class .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Accessory_Crystals
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Accessory_Crystals> rdf:type owl:Class ;
                                                                        rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicrole/Crystal_Role> ;
                                                                        rdfs:comment "Granular material role for constituent that forms less than about 5 percent of a Rock Material"@en ;
                                                                        rdfs:label "accessory constituent"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Block
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Block> rdf:type owl:Class ;
                                                           rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicrole/Inclusion> ;
                                                           <http://purl.org/dc/terms/source> "CGI rockbodypart vocabulary 2016-11-21 "@en ;
                                                           rdfs:comment "Geologic unit part is one or more masses with generally sharp boundaries and block-like geometry within a matrix of some other material emplaced by processes at the earth's surface--e.g. volcanic eruption or mass wasting. Use for objects that are larger that what would be considered a clast, e.g. greater than about 10 m shortest dimension, typically large enough and sparse enough to be considered individual objects."@en ;
                                                           rdfs:label "blocks"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Cataclastic_Matrix
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Cataclastic_Matrix> rdf:type owl:Class ;
                                                                        rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicrole/Matrix> ;
                                                                        rdfs:comment "rock consisting of greater than 50% sulfide or sulfosalt minerals formed by sedimentary exhalative processes." ;
                                                                        rdfs:label "Matrix formed by deformation "@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Cement
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Cement> rdf:type owl:Class ;
                                                            rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicrole/Matrix> ;
                                                            rdfs:comment "mineral material introduced during diagenesis that binds detrital material togther in a clastic rock. "@en ;
                                                            rdfs:label "cement "@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Clast
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Clast> rdf:type owl:Class ;
                                                           rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/common/Part> ,
                                                                           [ rdf:type owl:Restriction ;
                                                                             owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasRoleObject> ;
                                                                             owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/geology/Rock_Body>
                                                                           ] ,
                                                                           [ rdf:type owl:Restriction ;
                                                                             owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasRolePlayer> ;
                                                                             owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/geology/Solid_Geologic_Material>
                                                                           ] ;
                                                           rdfs:comment "Role for any fragment of material that is a constituent in a rock material; applies to fragments formed before lithification (e.g. sedimentary clasts) as well as fragments formed by in-place deformation of pre-existing rock (e.g. cataclastic fragments)."@en ;
                                                           rdfs:label "Clast"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Crystal_Role
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Crystal_Role> rdf:type owl:Class ;
                                                                  rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/common/Role> ,
                                                                                  [ rdf:type owl:Restriction ;
                                                                                    owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasRoleObject> ;
                                                                                    owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/geology/Rock_Body>
                                                                                  ] ,
                                                                                  [ rdf:type owl:Restriction ;
                                                                                    owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasRolePlayer> ;
                                                                                    owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/geology/Mineral>
                                                                                  ] ;
                                                                  rdfs:comment "constituent formed by crystallization from melt or by metamorphic processes. Distinct from constituent roles related to clastic processes, or diagenetic processes."@en ;
                                                                  rdfs:label "Crystal Constituent"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Enclave
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Enclave> rdf:type owl:Class ;
                                                             rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicrole/Inclusion> ;
                                                             <http://purl.org/dc/terms/source> "Cobbing, 2000, Barbarin, 1991"@en ;
                                                             rdfs:comment "General term for a polymineralic aggregate enclosed in a granitoid. Use when origin of inclusion as xenolith or a separate igneous phase is unclear."@en ;
                                                             rdfs:label "enclave"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Floating_Clast
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Floating_Clast> rdf:type owl:Class ;
                                                                    rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicrole/Clast> ;
                                                                    rdfs:comment "Clastic particles that are embedded in a sedimentary matrix in the original depositional process, e.g. clasts in a matrix-supported debris flow "@en ;
                                                                    rdfs:label "floating clast "@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Framework_Clast
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Framework_Clast> rdf:type owl:Class ;
                                                                     rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicrole/Clast> ;
                                                                     rdfs:comment "Clasts are part of an interconnected network of clasts in a sedimentary rock.  Conceptual model is the accumulation of clastic particles as originally deposited-- a heap of particles without matrix constituents. "@en ;
                                                                     rdfs:label "framework clast "@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Framework_Crystals
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Framework_Crystals> rdf:type owl:Class ;
                                                                        rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicrole/Crystal_Role> ;
                                                                        rdfs:comment "crystal that form the main framework of an igneous or metamorphic rock. Framework is the essential supporting structure of a rock material, formed by a collection of particles all in mutual contact to form a continuous connected framerwork; the framework commonly comprises more that one mineral phase, represented as different granular materials in this model, all in the 'framework crystal' role."@en ;
                                                                        rdfs:label "framework crystals"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Geologic_Unit_Matrix
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Geologic_Unit_Matrix> rdf:type owl:Class ;
                                                                          rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicrole/Matrix> ;
                                                                          <http://purl.org/dc/terms/source> "modified from CGI rockbodypart vocabulary 2016-11-21"@en ;
                                                                          rdfs:comment "Part of a geologic unit that is generally interstitial to other constituents, e.g. in a mass wasting deposit, melange, tuff breccia."@en ;
                                                                          rdfs:label "geologic unit matrix"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Groundmass
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Groundmass> rdf:type owl:Class ;
                                                                rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicrole/Matrix> ;
                                                                rdfs:comment "in a porphyritic or porphyroblastic rock, the finer-grained cystalline material between the larger crystals "@en ;
                                                                rdfs:label "groundmass"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Holoblast
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Holoblast> rdf:type owl:Class ;
                                                               rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicrole/Crystal_Role> ;
                                                               <http://purl.org/dc/terms/source> "Neuendorf et al., 2005"@en ;
                                                               rdfs:comment "A crystalline grain in a metamorphic rock that is newly and completely formed during metamorphism"@en ,
                                                                            "Might be a framework or accessory constituent"@en ;
                                                               rdfs:label "holoblast"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Hydrothermal_Vein
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Hydrothermal_Vein> rdf:type owl:Class ;
                                                                       rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicrole/Intrusive_Sheet> ;
                                                                       <http://purl.org/dc/terms/source> "Neuendorf et al 2005, CGI GTWG vocabulary"@en ;
                                                                       rdfs:comment "A tabular or sheet-like part of a compound material formed by hydrothermal (or other metasomatic) mineral filling a fracture, may be associated with replacement of the host rock adjacent to the body"@en ;
                                                                       rdfs:label "hydrothermal vein"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Igneous_Vein
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Igneous_Vein> rdf:type owl:Class ;
                                                                  rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicrole/Intrusive_Sheet> ;
                                                                  <http://purl.org/dc/terms/source> "CGI GTWG vocabulary"@en ;
                                                                  rdfs:comment "A tabular or sheet like part of a compound material formed by the intrusion of magma. Syn dike"@en ;
                                                                  rdfs:label "igneous vein"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Inclusion
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Inclusion> rdf:type owl:Class ;
                                                               rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/common/Role> ,
                                                                               [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasRoleObject> ;
                                                                                 owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/geology/Rock_Body>
                                                                               ] ,
                                                                               [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasRolePlayer> ;
                                                                                 owl:someValuesFrom [ rdf:type owl:Class ;
                                                                                                      owl:unionOf ( <http://loop3d.org/GSO/ontology/2020/1/geology/Geologic_Material>
                                                                                                                    <http://loop3d.org/GSO/ontology/2020/1/geology/Geologic_Object>
                                                                                                                  )
                                                                                                    ]
                                                                               ] ;
                                                               <http://purl.org/dc/terms/source> "CGI rockbodypart vocabulary 2016-11-21 "@en ;
                                                               rdfs:comment "A body of material present as masses with generally sharp boundaries enclosed within a matrix of some other material."@en ;
                                                               rdfs:label "Inclusion"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Intrusive_Sheet
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Intrusive_Sheet> rdf:type owl:Class ;
                                                                     rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/common/Part> ,
                                                                                     [ rdf:type owl:Restriction ;
                                                                                       owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasRoleObject> ;
                                                                                       owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/geology/Rock_Body>
                                                                                     ] ,
                                                                                     [ rdf:type owl:Restriction ;
                                                                                       owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasRolePlayer> ;
                                                                                       owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/geology/Rock_Body>
                                                                                     ] ;
                                                                     <http://purl.org/dc/terms/source> "CGI GTWG vocabualry"@en ;
                                                                     rdfs:comment "A tabular or sheet-like part of a rock body, genetic origin includes Intrusion_Process. Includes dikes, sills, and hydrothermal veins."@en ;
                                                                     rdfs:label "Intrusive sheet"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Main_Body
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Main_Body> rdf:type owl:Class ;
                                                               rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/common/Part> ,
                                                                               [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasRoleObject> ;
                                                                                 owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/geology/Rock_Body>
                                                                               ] ,
                                                                               [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasRolePlayer> ;
                                                                                 owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/geology/Rock_Body>
                                                                               ] ;
                                                               rdfs:comment "Role played by rock body part or constituent that forms most of the unit, and is host to parts in other roles."@en ;
                                                               rdfs:label "Main body role"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Matrix
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Matrix> rdf:type owl:Class ;
                                                            rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/common/Part> ,
                                                                            [ rdf:type owl:Restriction ;
                                                                              owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasRoleObject> ;
                                                                              owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/geology/Rock_Body>
                                                                            ] ,
                                                                            [ rdf:type owl:Restriction ;
                                                                              owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasRolePlayer> ;
                                                                              owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/geology/Solid_Geologic_Material>
                                                                            ] ;
                                                            rdfs:comment "Material in which something is enclosed or embedded (3b, https://www.merriam-webster.com/dictionary/matrix)"@en ;
                                                            rdfs:label "Matrix"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Nodule
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Nodule> rdf:type owl:Class ;
                                                            rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/common/Part> ,
                                                                            [ rdf:type owl:Restriction ;
                                                                              owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasRoleObject> ;
                                                                              owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/geology/Rock_Body>
                                                                            ] ,
                                                                            [ rdf:type owl:Restriction ;
                                                                              owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasRolePlayer> ;
                                                                              owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/geology/Rock_Body>
                                                                            ] ;
                                                            <http://purl.org/dc/terms/source> "Neuendorf et al., 2005"@en ;
                                                            rdfs:comment "An irregularly rounded mass of a mineral or mineral aggregate normally having a warty or knobby surface and no internal structure, usually with a contrasting composition from the enclosing sediment or rock matrix in which it is embedded, and that can be separated as a discrete mass from the host material."@en ;
                                                            rdfs:label "Nodule"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Orb
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Orb> rdf:type owl:Class ;
                                                         rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/common/Part> ;
                                                         <http://purl.org/dc/terms/source> "CGI GTWG vocabulary"@en ;
                                                         rdfs:comment "A circular-shaped rock body within another rock body."@en ,
                                                                      "Igneous constituent typically mafic, equant rounded spheroid with concentric mineralogic banding."@en ;
                                                         rdfs:label "Orb"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Overgrowth
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Overgrowth> rdf:type owl:Class ;
                                                                rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/common/Role> ,
                                                                                [ rdf:type owl:Restriction ;
                                                                                  owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasRoleObject> ;
                                                                                  owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/geology/Mineral>
                                                                                ] ,
                                                                                [ rdf:type owl:Restriction ;
                                                                                  owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasRolePlayer> ;
                                                                                  owl:someValuesFrom [ rdf:type owl:Class ;
                                                                                                       owl:unionOf ( <http://loop3d.org/GSO/ontology/2020/1/geology/Mineral>
                                                                                                                     <http://loop3d.org/GSO/ontology/2020/1/geology/Rock_Grain_Material>
                                                                                                                   )
                                                                                                     ]
                                                                                ] ;
                                                                rdfs:comment "A rock body part that encloses some other constituent in the rock body" ;
                                                                rdfs:label "Overgrowth" .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Paleoblast
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Paleoblast> rdf:type owl:Class ;
                                                                rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicrole/Crystal_Role> ;
                                                                <http://purl.org/dc/terms/source> "Neuendorf et al. 2005"@en ;
                                                                rdfs:comment "A crystal or remnant of a crystal that is older than other mineral grains in the rock, especially in metamorphic rocks"@en ,
                                                                             "Might be a framework or accessory constituent"@en ;
                                                                rdfs:label "paleoblast"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Paramorph
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Paramorph> rdf:type owl:Class ;
                                                               rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicrole/Pseudomorph> ;
                                                               <http://purl.org/dc/terms/source> "Neuendorf et al., 2005"@en ;
                                                               rdfs:comment "A pseudomorph with the same composition as the original crystal (eg: calcite after aragonite)"@en ;
                                                               rdfs:label "paramorph"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Pendant
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Pendant> rdf:type owl:Class ;
                                                             rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/common/Part> ,
                                                                             [ rdf:type owl:Restriction ;
                                                                               owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasRoleObject> ;
                                                                               owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/geology/Rock_Body>
                                                                             ] ,
                                                                             [ rdf:type owl:Restriction ;
                                                                               owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasRolePlayer> ;
                                                                               owl:someValuesFrom <http://loop3d.org/GSO/ontology/2020/1/geology/Rock_Body>
                                                                             ] ;
                                                             <http://purl.org/dc/terms/source> "Base on Neuendorf et al 2005"@en ;
                                                             rdfs:comment "A block of wall rock material in an igneous intrusion. Pendants become xenoliths as the dimension becomes smaller than about 10 m in their longest dimension. Although term pendant has connotation of being suspended or supported from above, this is rarely demonstrable in geologic situations, and the concept here does not require connection to the wall of the containing intrusion."@en ,
                                                                          "a mass of country rock that is entirely surrounded by an igneous intrusion such as a batholith or other pluton. "@en ;
                                                             rdfs:label "Pendant "@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Phenocryst
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Phenocryst> rdf:type owl:Class ;
                                                                rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicrole/Crystal_Role> ;
                                                                rdfs:comment "crystals that are larger than the enclosing groundmass in an igneous rock; typically euhedral. Phenocrysts do not play a framework role."@en ;
                                                                rdfs:label "phenocryst"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Porphyroblast
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Porphyroblast> rdf:type owl:Class ;
                                                                   rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicrole/Holoblast> ;
                                                                   <http://purl.org/dc/terms/source> "https://en.wikipedia.org/wiki/Porphyroblast" ;
                                                                   rdfs:comment "a large mineral crystal in a metamorphic rock which has grown within the finer grained matrix. Porphyroblasts are commonly euhedral crystals, but can also be partly to completely irregular in shape"@en ;
                                                                   rdfs:label "Porphyroblast "@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicrole/Porphyroclast
<http://loop3d.org/GSO/ontology/2020/1/geologicrole/Porphyroclast> rdf:type owl:Class ;
                                                                   rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/geologicrole/Paleoblast> ;
