                                                                                                                                                           owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasRole> ;
                                                                                                                                                           owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                                                                                                                                owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasQuality> ;
                                                                                                                                                                                owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                                                                                                                                                     owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasValue> ;
                                                                                                                                                                                                     owl:allValuesFrom <http://loop3d.org/GSO/ontology/2020/1/common/Dominant_Proportion>
                                                                                                                                                                                                   ]
                                                                                                                                                                              ]
                                                                                                                                                         ]
                                                                                                                                                         [ rdf:type owl:Restriction ;
                                                                                                                                                           owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/isOutputOf> ;
                                                                                                                                                           owl:someValuesFrom [ rdf:type owl:Class ;
                                                                                                                                                                                owl:unionOf ( <http://loop3d.org/GSO/ontology/2020/1/geology/Geologic_Event>
                                                                                                                                                                                              <http://loop3d.org/GSO/ontology/2020/1/geology/Geologic_Process>
                                                                                                                                                                                            )
                                                                                                                                                                              ]
                                                                                                                                                         ]
                                                                                                                                                       ) ;
                                                                                                                                    rdf:type owl:Class
                                                                                                                                  ]
                                                                                                             ]
                                                                                                           ) ;
                                                                                        rdf:type owl:Class
                                                                                      ] ;
                                                                  rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/common/Amount_Of_Matter> ,
                                                                                  [ rdf:type owl:Restriction ;
                                                                                    owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasPart> ;
                                                                                    owl:someValuesFrom [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                                                                owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasRole> ;
                                                                                                                                owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                                                                                                     owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasQuality> ;
                                                                                                                                                     owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                                                                                                                          owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/hasValue> ;
                                                                                                                                                                          owl:allValuesFrom <http://loop3d.org/GSO/ontology/2020/1/common/Dominant_Proportion>
                                                                                                                                                                        ]
                                                                                                                                                   ]
                                                                                                                              ]
                                                                                                                              [ rdf:type owl:Restriction ;
                                                                                                                                owl:onProperty <http://loop3d.org/GSO/ontology/2020/1/common/isOutputOf> ;
                                                                                                                                owl:someValuesFrom [ rdf:type owl:Class ;
                                                                                                                                                     owl:unionOf ( <http://loop3d.org/GSO/ontology/2020/1/geology/Geologic_Event>
                                                                                                                                                                   <http://loop3d.org/GSO/ontology/2020/1/geology/Geologic_Process>
                                                                                                                                                                 )
                                                                                                                                                   ]
                                                                                                                              ]
                                                                                                                            ) ;
                                                                                                         rdf:type owl:Class
                                                                                                       ]
                                                                                  ] ;
                                                                  owl:disjointWith <http://loop3d.org/GSO/ontology/2020/1/geology/Geologic_Object> ;
                                                                  rdfs:comment "An amount of matter primarily (dominantly) having parts created by geologic processes. Can include minor parts that are not of geologic origin, such as organic material"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geology/Geologic_Object
